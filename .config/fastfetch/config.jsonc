{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "auto",
    "source": "~/Pictures/onefetch/asuka.png",
    "width": 40,
    "preserveAspectRatio": true
  },
  "display": {
    "separator": ": ",
    "temp": {
      "unit": "C"
    },
    "key": {
      "type": "both"
    },
    "size": {
      "binaryPrefix": "jedec"
    }
  },
  "modules": [
    {
      "type": "title"
    },
    {
      "type": "separator"
    },
    {
      "type": "os",
      "key": "OS",
      "keyColor": "yellow",
      "keyIcon": "󱄅"
    },
    {
      "type": "users",
      "key": "Users",
      "keyColor": "yellow"
    },
    {
      "type": "kernel",
      "key": "Kernel",
      "keyColor": "yellow"
    },
    {
      "type": "uptime",
      "key": "Uptime",
      "keyColor": "yellow"
    },
    {
      "type": "separator"
    },
    {
      "type": "de",
      "key": "DE",
      "keyColor": "green"
    },
    {
      "type": "wm",
      "key": "WM",
      "keyColor": "green"
    },
    {
      "type": "theme",
      "key": "Theme",
      "keyColor": "green"
    },
    {
      "type": "icons",
      "key": "Icons",
      "keyColor": "green",
      "keyIcon": "󰓎"
    },
    {
      "type": "terminal",
      "key": "Terminal",
      "keyColor": "green"
    },
    {
      "type": "separator"
    },
    {
      "type": "cpu",
      "key": "CPU",
      "showPeCoreCount": true,
      "keyColor": "magenta"
    },
    {
      "type": "gpu",
      "key": "GPU",
      "keyColor": "magenta"
    },
    {
      "type": "memory",
      "key": "Memory",
      "keyColor": "magenta"
    },
    {
      "type": "separator"
    },
    {
      "type": "media"
    },
    {
      "type": "separator"
    },
    {
      "type": "colors",
      "symbol": "square"
    }
  ]
}
