{
  "rpc.detailsEditing": "Editing: {file_name}:{current_line}:{current_column}",
  "rpc.detailsViewing": "Viewing: {file_name}",
  "rpc.lowerDetailsEditing": "📂 {workspace_folder}",
  "rpc.lowerDetailsViewing": "📁 {workspace_folder}",
  "rpc.lowerDetailsNotFound": "No workspace",
  "codeLens.enable": true,
  "elixir.pathToElixirLS": "~/.elixir-ls/release/language_server.sh",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  }
}
